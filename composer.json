{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/ckeditor": "4.11.0",
    "craftcms/cms": "5.8.21",
    "nystudio107/craft-seomatic": "5.1.19",
    "samuelreichor/craft-query-api": "*",
    "spacecatninja/imager-x": "5.1.7",
    "verbb/cloner": "3.0.2",
    "verbb/hyper": "2.2.10",
    "verbb/navigation": "3.0.15",
    "vlucas/phpdotenv": "5.6.1"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "path",
      "url": "plugins/query-api"
    },
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ],
  "autoload": {
    "psr-4": {
      "modules\\queryapiextension\\": "modules/query-api-extension/"
    }
  }
}
